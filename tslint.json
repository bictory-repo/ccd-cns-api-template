{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "no-magic-numbers": false,
        "no-parameter-reassignment": true,
        "no-reference": false,
        "no-trailing-whitespace": false,
        "no-console": {
            "severity": "error",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "promise-function-async": true,
        "no-namespace": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}